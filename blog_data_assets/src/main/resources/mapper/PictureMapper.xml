<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.blogDataAssets.mapper.PictureMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.blog.blogDataAssets.po.Picture">
        <id column="id" property="id" />
        <result column="pictureaddress" property="pictureaddress" />
        <result column="picturedescription" property="picturedescription" />
        <result column="picturename" property="picturename" />
        <result column="picturetime" property="picturetime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pictureaddress, picturedescription, picturename, picturetime
    </sql>

</mapper>
